<p id="notice"><%= notice %></p>

<h1>Friend List</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Twitter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friendsses.each do |friendss| %>
      <tr>
        <td><%= friendss.first_name %></td>
        <td><%= friendss.last_name %></td>
        <td><%= friendss.email %></td>
        <td><%= friendss.phone %></td>
        <td><%= friendss.twitter %></td>
        <td><%= link_to 'Show', friendss %></td>
        <td><%= link_to 'Edit', edit_friendss_path(friendss) %></td>
        <td><%= link_to 'Destroy', friendss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friend', new_friendss_path %>
